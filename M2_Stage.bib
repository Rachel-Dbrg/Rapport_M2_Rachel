  author = {{van den Beld}, Inge M. J. and Bourillet, Jean-Fran{\c c}ois and {Arnaud-Haond}, Sophie and {de Chambure}, Laurent and Davies, Jaime S. and Guillaumont, Brigitte and Olu, Karine and Menot, L{\'e}na{\"i}ck},
  year = {2017},
