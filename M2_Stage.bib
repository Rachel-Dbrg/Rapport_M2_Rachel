@article{natarajan2016,
  title = {Predictable Convergence in Hemoglobin Function Has Unpredictable Molecular Underpinnings},
  author = {Natarajan, Chandrasekhar and Hoffmann, Federico G. and Weber, Roy E. and Fago, Angela and Witt, Christopher C. and Storz, Jay F.},
  year = {2016},
  month = oct,
