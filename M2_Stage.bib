  author = {Vilgrain, Laure and Maps, Fr{\'e}d{\'e}ric and Picheral, Marc and Babin, Marcel and Aubry, Cyril and Irisson, Jean-Olivier and Ayata, Sakina-Doroth{\'e}e},
  volume = {n/a},
  issn = {1939-5590},
  doi = {10.1002/lno.11672},
  abstract = {Imaging techniques are increasingly used in ecology studies, producing vast quantities of data. Inferring functional traits from individual images can provide original insights on ecosystem processes. Morphological traits are, as other functional traits, individual characteristics influencing an organism's fitness. We measured them from in situ image data to study an Arctic zooplankton community during sea ice break-up. Morphological descriptors (e.g., area, lightness, complexity) were automatically measured on {$\sim$} 28,000 individual copepod images from a high-resolution underwater camera deployed at more than 150 sampling sites across the ice-edge. A statistically-defined morphological space allowed synthesizing morphological information into interpretable and continuous traits (size, opacity, and appendages visibility). This novel approach provides theoretical and methodological advantages because it gives access to both inter- and intra-specific variability by automatically analyzing a large dataset of individual images. The spatial distribution of morphological traits revealed that large copepods are associated with ice-covered waters, while open waters host smaller individuals. In those ice-free waters, copepods also seem to feed more actively, as suggested by the increased visibility of their appendages. These traits distributions are likely explained by bottom-up control: high phytoplankton concentrations in the well-lit open waters encourages individuals to actively feed and stimulates the development of small copepod stages. Furthermore, copepods located at the ice edge were opaquer, presumably because of full guts or an increase in red pigmentation. Our morphological trait-based approach revealed ecological patterns that would have been inaccessible otherwise, including color and posture variations of copepods associated with ice-edge environments in Arctic ecosystems.},
  annotation = {\_eprint: https://aslopubs.onlinelibrary.wiley.com/doi/pdf/10.1002/lno.11672},
  file = {C\:\\Users\\rache\\Zotero\\storage\\8GDMI794\\Vilgrain et al. - Trait-based approach using in situ copepod images .pdf;C\:\\Users\\rache\\Zotero\\storage\\TYR38IW8\\lno.11672.pdf;C\:\\Users\\rache\\Zotero\\storage\\D4XP7F9H\\lno.html},
  journal = {Limnology and Oceanography},
  language = {en},
  number = {n/a}
}

@article{villeger2011,
