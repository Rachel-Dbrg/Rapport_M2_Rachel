@article{garcia2021,
  title = {Biogeographical Patterns of Meso- and Bathypelagic Fish along a {{Northeastern Atlantic}} Transect},
  author = {{Garc{\'i}a-Seoane}, Eva and Wienerroither, Rupert and Mork, Kjell Arne and Underwood, Melanie J and Melle, Webj{\o}rn},
  year = {2021},
  month = mar,
  issn = {1054-3139},
  doi = {10.1093/icesjms/fsaa255},
  abstract = {The influence of oceanographic variables on assemblages of meso- and bathypelagic fish was investigated along a Northeastern Atlantic Ocean transect (Cape Verde to the Bay of Biscay) during May 2019. Fish were collected using a mrozooplankton trawl during daylight hours at ten stations. Along the transect, 17 hydrographic stations were also performed with a CTD (Conductivity, Temperature, and Depth). A total of 130 fish taxa were identified. The dominant family was Gonostomatidae, with four species (Cyclothone braueri, Cyclothone microdon, Cyclothone pseudopallida, and Cyclothone pallida) being responsible of more than 78 \% of the total density. The most frequent species that appeared to be ubiquitous were C. braueri and C. pseudopallida, while Myctophidae was the most diverse family. Multivariate analyses revealed two clusters related with the latitudinal gradient. The fish community in the southern stations (25\textendash 37\textdegree N) was more diverse than in the northern stations (42\textendash 48\textdegree N). Temperature from 300 to 700 m depth explained 65 \% of variation in terms of density and 58 \% in terms of biomass, both statistically significant. The investigated variation in the deep-pelagic ecosystems on a large spatial scale gives essential information to ecosystem management approhes and marine spatial planning.},
  eprint = {https://academic.oup.com/icesjms/advance-article-pdf/doi/10.1093/icesjms/fsaa255/36588060/fsaa255.pdf},
  journal = {ICES Journal of Marine Science},
  note = {fsaa255}
}

