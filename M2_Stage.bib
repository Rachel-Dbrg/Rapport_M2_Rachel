@article{beauchard2017,
  title = {The Use of Multiple Biological Traits in Marine Community Ecology and Its Potential in Ecological Indicator Development},
  author = {Beauchard, O. and Ver{\'i}ssimo, H. and Queir{\'o}s, A. M. and Herman, P. M. J.},
  year = {2017},
  month = may,
  volume = {76},
  pages = {81--96},
  issn = {1470-160X},
  doi = {10.1016/j.ecolind.2017.01.011},
  abstract = {Biological traits offer valuable approaches to understand species distributions and underlying mechanisms. Their use has received a growing interest in marine community ecology, for both fundamental and applied purposes. The need of ecological indicators as part of marine directives and conservation programmes has promoted the use of multiple traits for indicator development, but in a questionable context regarding the state of fundamental developments. Biological Trait Analysis (BTA) is a complex research field, characterised by flexible concepts and applications. In order to enhance the development of relevant marine ecological indicators, this review provides baselines for better theoretical and applied BTA. A compilation of the existing literature reveals that specific topics have dominated the use of multiple traits in marine ecology unlike in freshwater and terrestrial ecology where tests of theories and uses of evolutionary concepts consistently preceded BTA applications. Availability of data sets and analytical techniques seemed to have driven the growing use of marine BTA rather than fundamental questions regarding life history theories in marine ecosystem components and the functional nature of traits. It is therefore suggested that greater focus on life history ecology and on the links between marine species traits and ecosystem functioning are still needed to support trait-based indicator development. Life history strategy understanding is put forward as a theoretically-sound basis and fundamental pre-requisite for trait-based marine indicator development.},
  file = {C\:\\Users\\rache\\Zotero\\storage\\4DHQ57B4\\1-s2.0-S1470160X17300110-main.pdf;C\:\\Users\\rache\\Zotero\\storage\\8MFLSLJZ\\Beauchard et al. - 2017 - The use of multiple biological traits in marine co.pdf;C\:\\Users\\rache\\Zotero\\storage\\ISNCSJHU\\S1470160X17300110.html},
  journal = {Ecological Indicators},
  keywords = {Biological traits,Ecological indicators,Marine environment},
  language = {English}
}

