@article{jaime2015,
  title = {Complex Patterns of Environmental Niche Evolution in Iberian Columbines (Genus Aquilegia, Ranunculaceae)},
  author = {Jaime, Rafael and Alc{\'a}ntara, Julio M. and Bastida, Jes{\'u}s M. and Rey, Pedro J.},
