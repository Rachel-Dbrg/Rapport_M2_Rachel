  shorttitle = {Realizing the Potential of Trait-Based Aquatic Ecology},
  author = {Kremer, Colin T. and Williams, Alicia K. and Finiguerra, Michael and Fong, Allison A. and Kellerman, Anne and Paver, Sara F. and Tolar, Bradley B. and Toscano, Benjamin J.},
  year = {2017},
  volume = {62},
  pages = {253--271},
  issn = {1939-5590},
  doi = {10.1002/lno.10392},
  abstract = {Trait-based ecology, which focuses on using the traits of species and individuals to understand ecology (from populations to ecosystems), is becoming an increasingly productive and widely employed paradigm. To date, trait-based approaches have been used to study taxa from microbes to megafauna in every major area of aquatic ecology yielding exciting results. However, this promising field faces a number of significant obstacles, including: (1) identifying and measuring ecologically relevant traits, (2) integrating inter- and intra-specific trait variation, (3) detecting and quantifying trait correlations and trade-offs, and (4) accounting for the context dependency of traits. These issues are often particularly acute for specific taxa or systems. This paper highlights these looming challenges, as well as ways to address them. Proposed solutions center around using new technologies to collect trait data, coordinating research efforts, and curating and sharing data. Throughout, we take an interdisciplinary approach, sharing examples spanning a wide range of aquatic taxa and systems. It is our hope that this paper will stimulate frank discussions and help the growing field of trait-based aquatic ecology maximize its potential.},
  annotation = {\_eprint: https://aslopubs.onlinelibrary.wiley.com/doi/pdf/10.1002/lno.10392},
  file = {C\:\\Users\\rache\\Zotero\\storage\\WJDL45EN\\Kremer et al. - 2017 - Realizing the potential of trait-based aquatic eco.pdf;C\:\\Users\\rache\\Zotero\\storage\\XDAE982T\\Kremer et al. - 2017 - Realizing the potential of trait-based aquatic eco.pdf;C\:\\Users\\rache\\Zotero\\storage\\DYD3AN4D\\lno.html},
  journal = {Limnology and Oceanography},
  language = {en},
  number = {1}
}

